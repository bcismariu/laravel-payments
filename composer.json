{
    "name": "bcismariu/laravel-payments",
    "description": "An alternative to Cashier",
    "keywords": ["laravel", "billing", "konnektive"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bogdan Cismariu",
            "email": "bogdan.cismariu@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bcismariu\\Laravel\\Payments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bcismariu\\Laravel\\Payments\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "bcismariu/commons-descendable": "^0.1",
        "illuminate/support": "^5.4",
        "nesbot/carbon": "~1.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bcismariu\\Laravel\\Payments\\PaymentsServiceProvider"
            ],
        }
    }
}
